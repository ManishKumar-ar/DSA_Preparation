#include <iostream>
using namespace std;
int main()
{
    int arr[5] = {2, 4, 6, 8, 10};
    int count = 0;
    for (int i = 0; i < 5; i++)
    {
        for (int j = i ; j < 5; j++)
        {
            for (int k = i; k<=j; k++)
            {
                cout<<arr[k]<<" ";
               
            }
              cout<<endl;
               count++;
        }
        cout<<endl;
    }
    cout << "\ntotal number of pairs " << count;
}




//output 

2 
2 4 
2 4 6 
2 4 6 8 
2 4 6 8 10 

4 
4 6 
4 6 8 
4 6 8 10 

6 
6 8 
6 8 10 

8 
8 10 

10 


total number of pairs 15
