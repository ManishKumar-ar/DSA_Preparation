#include <iostream>
using namespace std;

int main()
{
    int size = 5;
    int F[size] = {-1};
    F[0] = 0;
    F[1] = 1;
    int i;
    for (i = 2; i < size; i++)
    {
        F[i] = F[i - 2] + F[i - 1];
    }

    cout <<" nth term is " <<F[i - 1] << endl;
    //  or
    cout <<" nth term is " <<F[size - 1];
}


// output 
// nth term is 3
// nth term is 3
